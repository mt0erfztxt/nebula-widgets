@require 'knob';


.manPage-interactiveExample
    &__knob
        border: 1px solid $g_color_primary_text--disabled;
        display: table-row-group;


    &__knobs
        border: 1px solid $g_color_primary_text--disabled;
        border-collapse: collapse;
        display: table;
        width: 100%;


    &__knobs-header
        display: table-cell
        font-weight: bold;
        padding: 8px;
        text-align: center;


    &__knobs-headers
        background-color: $g_color_background--darker;
        border: 1px solid $g_color_primary_text--disabled;
        display: table-header-group;


    &__view
        margin-bottom: 16px;
