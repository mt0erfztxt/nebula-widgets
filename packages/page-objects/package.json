{
  "name": "nebula-widgets-page-objects",
  "version": "0.2.0",
  "description": "Widgets",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mt0erfztxt/nebula-widgets.git"
  },
  "author": "Sergey Kozhevnikov <mt0erfztxt@gmail.com>",
  "main": "lib",
  "files": [
    "lib"
  ],
  "directories": {
    "test": "src/test"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "compile": "npx babel --copy-files --extensions '.js' --ignore 'src/fragments/**/*,src/kitchen-sink/**/*,src/test/spec/**/*,src/widgets/**/*' --out-dir lib src --source-maps",
    "compile-watch": "npm run compile -- --watch",
    "test": "npm run testcafe-tests",
    "testcafe-tests": "npm run clean && npm run compile && npx testcafe --selector-timeout=3000 chromium lib/test/testcafe"
  },
  "dependencies": {
    "@sindresorhus/is": "1.2.0",
    "app-root-path": "2.2.1",
    "nebula-test-fragment": "file:../../../nebula-test-fragment-0.3.0.tgz",
    "testcafe": "1.8.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-proposal-private-methods": "7.8.3",
    "@babel/preset-env": "7.8.6",
    "@babel/register": "7.8.3",
    "@types/app-root-path": "1.2.4",
    "prettier": "1.19.1",
    "unexpected": "11.12.1"
  }
}
